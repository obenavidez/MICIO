<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bgdialog2"
    android:orientation="vertical" >

    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/White"
        android:gravity="center" >

        <TextView
            android:id="@+id/header"
            style="StyleFontTextViewActionBar"
            android:layout_width="fill_parent"
            android:layout_height="20dp"
            android:background="@drawable/bg_actionbar29"
            android:gravity="center"
            android:text="Costeo Devolución"
            android:textColor="@color/White" />

        <TableRow
            android:id="@+id/tableRow0"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:background="@drawable/style_textbox"
                android:text="Sistema"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:background="@drawable/style_textbox"
                android:text="Vendedor"
                android:textColor="@color/Teal"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/cxclbl_subtsys"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:text="SubTotal:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_subtsys"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_subtvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="SubTotal:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_subtvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/cxclbl_bonifsys"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:text="-Bonificación:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_bonifsys"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_bonifvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="-Bonificación:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_bonifvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/cxclbl_impsys"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:text="+Impuesto:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_impsys"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_impvend"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="false"
                android:layout_alignParentTop="false"
                android:layout_weight="1"
                android:text="+Impuesto:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_impvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/cxclbl_promsys"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:text="-Promoción:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctext_promsys"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_promvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="-Promoción:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctext_promvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/cxclbl_cargadmsys"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:text="-Cargo Adm:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_cargadmsys"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_cargadmvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="-Cargo Adm:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctextv_cargadmvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="10"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_vinievend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="-Viñeta:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctext_vinievend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/cxclbl_totalsys"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:text="Total:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctext_totalsys"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxclbl_totalvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Total :"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctext_totalvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:textColor="@color/Black"
                android:textSize="10dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_weight="1" />

            <TextView
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/cxclbl_cargvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Cargo Vendedor:"
                android:textColor="@color/Teal"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/cxctext_cargvend"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text=""
                android:textColor="@color/Teal"
                android:textSize="10dp" />
        </TableRow>
    </TableLayout>

</LinearLayout>